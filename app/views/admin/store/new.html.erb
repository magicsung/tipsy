<h3>NEW STORE</h3>

<%= form_for @store, :url => admin_store_index_path do |n|  %>

<%= render :partial => 'form', :locals => { :n => n } %>

<%= n.submit "Create", :class => "btn btn-primary" %>
<%= link_to "Cancel", admin_store_index_path, :class => "btn btn-info pull-right" %>

<div class="clearfix"></div>

<% end %>
<div class="page-header">
  <h1>Admin console</h1>
</div>


<div class="col-md-6">
  <table class="table table-hover">
    <tr class="info">
      <td>User name</td>
      <td>Email</td>
    </tr>
    <tr>
      <% User.all.each do |x| %>
        <td><%= x.name %></td>
        <td><%= x.email %></td>
      <% end %>
    </tr>
  </table>
</div>

<div class="page-header">
  <h1>User console</h1>
</div>


<div class="col-md-12">
  
  <ul class="nav nav-tabs">
    <li role="presentation"><%= link_to 'Store', admin_root_path %></li>
    <li role="presentation" class="active"><%= link_to 'User', admin_user_index_path %></li>
    <li role="presentation"><a href="#">Event</a></li>
    <li role="presentation"><a href="#">DJ</a></li>
  </ul>

  <table class="table table-hover">
    <tr>
      <th>User name</th>
      <th>Email</th>
    </tr>
      <% User.all.each do |x| %>
      <tr>
        <td><%= x.name %></td>
        <td><%= x.email %></td>
      <% end %>
    </tr>
  </table>
</div>

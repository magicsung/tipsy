<div class="page-header">
  <h1>User console</h1>
</div>


<div class="col-md-12">

  <table class="table table-hover">
    <tr>
      <th>User name</th>
      <th>Email</th>
    </tr>
      <% User.all.each do |x| %>
      <tr>
        <td><%= x.name %></td>
        <td><%= x.email %></td>
      <% end %>
    </tr>
  </table>
</div>


<div class="profile col-xs-12 col-md-4 col-md-offset-4">
  
  <div class="wrapper">
    <div class="user-image">
      <%= image_tag(current_user.image, :class => "img-circle", :id => "user-photo") %>
    </div>
    <table class="table">
    <tr>
      <th>Name:</th>
      <td><%= current_user.name %></td>
    </tr>
    <tr>
      <th>Email:</th>
      <td><%= current_user.email %></td>
    </tr>
    
    </table>
  </div>
  
  
  <div class="user-logout">
    <% if current_user %>
      <%= button_to('Log out', destroy_user_session_path, :method => :delete, :class => 'btn logout') %>
    <% end %>
  </div>

  <div class="empty100px"></div>


  <div class="qrcode">
    <%= image_tag @qr %>
  </div>

</div>


